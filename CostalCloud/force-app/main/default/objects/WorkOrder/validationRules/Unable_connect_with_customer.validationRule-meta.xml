<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unable_connect_with_customer</fullName>
    <active>true</active>
    <errorConditionFormula>AND (
 ( Unable_to_Connect_with_Customer__c  = TRUE) ,
 ISBLANK( Last_Contact_Attempt_Made__c ))</errorConditionFormula>
    <errorDisplayField>Last_Contact_Attempt_Made__c</errorDisplayField>
    <errorMessage>Please enter date/time of last attempt to connect with customer</errorMessage>
</ValidationRule>
